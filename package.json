{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "watch": {
    "compile": "public/src/tag/*.tag"
  },
  "scripts": {
    "start": "node ./bin/www",
    "babelify": "babel public/src/js --watch --out-file public/dist/js/bundle.js",
    "compile": "riot public/src/tag public/dist/tag/tag.es6.js",
    "compile_final": "babel --watch public/dist/tag/tag.es6.js --out-file public/dist/tag/tag.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-watch",
    "make": "concurrently \"npm run babelify\" \"npm run compile_final\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.4.0",
    "async": "^2.6.1",
    "babel-core": "^6.11.4",
    "babel-preset-es2015-riot": "^1.1.0",
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "csv-export": "^1.0.2",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-myconnection": "^1.0.4",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.18.1",
    "morgan": "~1.9.0",
    "multer": "^1.3.1",
    "mysql": "^2.16.0",
    "nodemailer": "^4.6.8",
    "npm-watch": "^0.3.0",
    "request": "^2.88.0",
    "riot": "^3.11.1",
    "riot-route": "^3.1.3",
    "riotcontrol": "0.0.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.3",
    "babel-cli": "^6.11.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "parallelshell": "^2.0.0"
  }
}
